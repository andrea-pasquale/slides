{
  "name": "vue-starport",
  "version": "0.3.0",
  "packageManager": "pnpm@7.2.1",
  "description": "Shared Vue component across routes with animations",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/vue-starport#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/vue-starport.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/vue-starport/issues"
  },
  "keywords": [
    "vue",
    "animation",
    "transition"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@vueuse/core": "^8.6.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@antfu/ni": "^0.16.2",
    "@types/node": "^17.0.43",
    "bumpp": "^7.2.0",
    "cypress": "^9.5.4",
    "eslint": "^8.17.0",
    "nanoid": "^4.0.0",
    "pkgroll": "^1.3.1",
    "pnpm": "^7.2.1",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vitest": "^0.15.1",
    "vue-router": "^4.0.16",
    "vue-tsc": "^0.37.8"
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch",
    "play": "vite playground --open",
    "play:build": "vite build playground",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:e2e": "cypress open",
    "release": "bumpp --commit --tag --push && pnpm publish"
  }
}